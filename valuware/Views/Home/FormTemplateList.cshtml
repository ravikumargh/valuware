@{
    Layout = null;
}

<!DOCTYPE html>
<html ng-app="drag-and-drop">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Project Work</title>
    <link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular.min.js"></script>

    <script src="~/Scripts/ngStorage.min.js"></script>
    <script src="~/Scripts/formtemplatelist.js"></script>
    
    <link rel="stylesheet" href="../../css/mainstyle.css" type='text/css'>
    <link rel="stylesheet" href="../../css/font-awesome.min.css" type='text/css'>
   
</head>

<body ng-controller="templateListController">
    <div class="wrapper">
        <!--Header-->
        <header>
            <div class="left">
                <button id="menu" title="Menu"><i class="fa fa-bars fa-2x"></i></button>
                <h1> <i class="fa fa-play"></i> </h1>
            </div>
            <div class="utils right"> <a href="/Home/FormDesigner" class='alerts'>New (show PLUS image)</a> <i class="sep">&nbsp;</i><a href="#"> &nbsp;<i class="fa fa-ellipsis-v"></i></a> </div>
        </header>
        <!--//Header-->

        <div style="display:none">
            <input type="checkbox" id="togglebox" />
            <nav id="offcanvas-menu"> Menu contents to be added </nav>
            <div id="contentarea">
                <label for="togglebox" id="navtoggler">Menu</label>
            </div>
        </div>
        <div class="col-md-6 mob_col-md-12" id="LeftInfo">
            <div class="left_bg">
                <div class="left_border">
                    <div class="bor_bot">
                        <div class="hide"> Hide </div>
                        <div class="pageH line_height"> Hellp Drogo, Welcome back, Its a busy day ahed with 8 requests</div>
                    </div>
                    <div class="page_sub bor_bot"> Performance previous VS Current month </div>
                    <div class="page_sub bor_bot">
                        Your Best Months is March 2015 as Infographic
                        <div class="Infographic">
                            <ul>
                                <li>
                                    <h1> 1 </h1>
                                    <div class="clear_line"> </div>
                                    <span> Canceled </span>
                                </li>
                                <li>
                                    <h1> 208 </h1>
                                    <div class="clear_line"> </div>
                                    <span> Finished </span>
                                </li>
                                <li>
                                    <h1> Hd3h </h1>
                                    <div class="clear_line"> </div>
                                    <span>Time Average </span>
                                </li>
                            </ul>
                        </div>
                        <div class="clear_line"> </div>
                    </div>
                    <div class="clear_line"> </div>
                    <div class="page_sub">
                        Monthly Statistics for August 2015
                        <div class="tab_calss"> <a href="#">Text here</a> <a href="#"> Text here</a> <a href="#"> Text here</a> <a class="tab_sel" href="#"> Text here</a> </div>
                        <div class="progress_tab">
                            <ul>
                                <li>
                                    <span> Abu Dhabi Islamic Bank </span>
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 45%;"> 25% </div>
                                    </div>
                                </li>
                                <li>
                                    <span> Noor Bank </span>
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 80%;"> 80% </div>
                                    </div>
                                </li>
                                <li>
                                    <span> National Bank of Fujairah </span>
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 30%;"> 30% </div>
                                    </div>
                                </li>
                                <li>
                                    <span> Mashreq Bank </span>
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 50%;"> 50% </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-6 mob_col-md-12" id="MainInfo" data-ng-init="loadAllFormTemplates();">
            <div class="filter_area">
                <div class="">
                    <i class="fa fa-search"></i>
                    <input type="text" name="firstname" class="search" placeholder=" Search here" />
                </div>
                <div class=""> <span class="filter"> filter </span></div>
            </div>
            <div class="content_div" ng-repeat='template in templatesList'>
                <div class="col-md-3"> Name Comes Here </div>
                <div class="col-md-6">
                    <div class="content_text"> {{template.form_Template_Name}} </div>
                    <div class="Townhouse"> Town house </div>
                    <div class="LastUpdated"> Last Updated:  Monday </div>
                </div>
                <div class="col-md-3 rightPadding">
                    <span class="view_btn right" ng-click="loadFormTemplateDetails(template.form_Template_ID)"> <a href="#"> View</a></span>
                    <div class="clear_line"> </div>
                    <span class="lostlink"> RE 1506 527032 </span>
                </div>
            </div>

        </div>
    </div>
    <script src="js/jquery-2.1.4.min.js"></script>
    <script>
        $(function () {
            $("#menu").on("click", function () {
                $(this).toggleClass("active");
                $("body").toggleClass("nav-open");
            });
        });
    </script>
</body>
</html>


